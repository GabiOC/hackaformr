<!DOCTYPE html>
<html>
<head>
  <title>Hackaformr</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= analytics_init if Rails.env.production? %>
</head>
<body>

<% unless current_page?(root_path) || current_page?(new_admin_session_path) || current_page?(new_admin_registration_path) || current_page?(new_user_session_path) || current_page?(new_user_registration_path) %>
  <%= render :partial => "header" %>
<% end %>
<%= render 'flash_messages' %>
<%= yield %>

</body>
</html>
